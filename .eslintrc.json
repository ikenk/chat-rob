{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "vue-eslint-parser", 
    "extends": [
        // "eslint:recommended",
        // "plugin:@typescript-eslint/recommended",
        // "plugin:vue/vue3-essential",
        "plugin:prettier/recommended"
        // "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "vue",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
    // "globals": {
    //     "defineProps": "readonly"
    // }
}
